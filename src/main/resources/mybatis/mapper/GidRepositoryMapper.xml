<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.pinkchampagne17.channelserver.repository.GidRepository">
    <insert
            id="createGid"
            parameterType="io.github.pinkchampagne17.channelserver.parameters.GidCreateParameters"
            useGeneratedKeys="true"
            keyProperty="gid"
    >
        insert into gid(link) values(#{link})
    </insert>
</mapper>
